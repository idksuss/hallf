local Section = ma:CreateSection("Auto Farm Quests")

local LoopSlider = ma:CreateSlider({
    Name = "Loop Delay (Secs)",
    Range = {1, 120},
    Increment = 1,
    Suffix = "Secs",
    CurrentValue = 20,
    Flag = "Loopslider",
